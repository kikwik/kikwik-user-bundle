{
    "name": "kikwik/user-bundle",
    "description": "A super simple user bundle that provide very basic helpers for symfony 4 user management",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Davide Scarpato",
            "email": "davide@kikwik.it"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/security-bundle": "^4.4|^5.0",
        "symfony/translation": "^4.4|^5.0",
        "symfony/mailer": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "stof/doctrine-extensions-bundle": "^1.3",
        "kikwik/iptraceable-listener-bundle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kikwik\\UserBundle\\": "src/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4|^5.0",
        "nyholm/symfony-bundle-test": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Kikwik\\UserBundle\\Tests\\": "tests/"
        }
    }
}
