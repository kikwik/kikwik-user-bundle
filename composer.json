{
    "name": "kikwik/user-bundle",
    "description": "A super simple user bundle that provide very basic helpers for symfony 4 user management",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Davide Scarpato",
            "email": "davide@kikwik.it"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "symfony/framework-bundle": "^6.4",
        "symfony/security-bundle": "^6.4",
        "symfony/translation": "^6.4",
        "symfony/mailer": "^6.4",
        "symfony/form": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "stof/doctrine-extensions-bundle": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Kikwik\\UserBundle\\": "src/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4",
        "nyholm/symfony-bundle-test": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Kikwik\\UserBundle\\Tests\\": "tests/"
        }
    }
}
